 #999898;\r\n}\r\n\r\n#DataDoLaudo {\r\n  width: 100%;\r\n  color: #999898;\r\n  padding-top: 55%;\r\n}\r\n\r\n#contentUpload {\r\n  width: 46%;\r\n  height: 100%;\r\n  border-radius: 8px;\r\n  background-color: #fff;\r\n  color: #2e4a7d;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n#carregarLaudo {\r\n  padding-top: 5%;\r\n}\r\n\r\n#linhaUpload {\r\n  width: 75%;\r\n  border: 0.1px solid #2e4a7d;\r\n  background-color: #2e4a7d;\r\n  margin-top: 2%;\r\n}\r\n\r\n#drap_in_drop {\r\n  width: 99%;\r\n  height: 50%;\r\n  background-color: #f5f5f5;\r\n  border: 3px dashed;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  padding-top: 3%;\r\n  margin-top: 4%;\r\n}\r\n\r\n\r\n#btnFile {\r\n  width: 45%;\r\n  height: 23%;\r\n  border-radius: 5px;\r\n  margin-top: 3%;\r\n  background-color: #2e4a7d;\r\n  cursor: pointer;\r\n  color: #fff;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  transition: 1s;\r\n}\r\n\r\n#btnFile:hover {\r\n  background-color: #ffd666;\r\n  color: #2e4a7d;\r\n}\r\n\r\n#imagem8 {\r\n  width: 14%;\r\n  padding-right: 3%;\r\n}\r\n\r\n#imagem7 {\r\n  width: 8%;\r\n  color: #2e4a7d;\r\n  margin-top: 4%;\r\n  padding-bottom: 1%;\r\n}\r\n\r\n#formUpload {\r\n  width: 76%;\r\n  color: #2e4a7d;\r\n  font-size: 20px;\r\n}\r\n\r\n#inputNome {\r\n  width: 100%;\r\n  height: 20%;\r\n  background-color: #f5f5f5;\r\n  display: flex;\r\n  flex-direction: column;\r\n  border-radius: 8px 8px 8px 0;\r\n  margin-top: 5%;\r\n  padding-top: 2%;\r\n}\r\n\r\n#nomeLaudo {\r\n  margin-left: 4%;\r\n}\r\n\r\n#Nome {\r\n  width: 88.7%;\r\n  height: 40%;\r\n  border-radius: 5px;\r\n  margin-left: 4%;\r\n  margin-top: 2%;\r\n  padding-left: 3%;\r\n}\r\n\r\n#Nome::placeholder {\r\n  color: #2e4a7d;\r\n  font-size: 15px;\r\n}\r\n\r\n#inputData {\r\n  width: 50%;\r\n  height: 25%;\r\n  background-color: #f5f5f5;\r\n  display: flex;\r\n  flex-direction: column;\r\n  border-radius: