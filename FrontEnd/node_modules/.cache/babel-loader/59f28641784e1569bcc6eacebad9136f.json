,KAAA,CAEL;AADA,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,eAAA,EAAF;AAAA,KAAA;AACN,GARF,E,IAAA,E;;AAAA,I,UAAA,C,oBAGU,KAAA,CAAA,K,GAeL,EARO,mBAAA,CAAA,0CAAA,CAQP,EAPL,mBAAA,CAMS,QANT,EAMS;AALR,IAAA,KAAkD,EAAlD;AAAA,eAAA,KAAA;AAAA,gBAAA,MAAA;AAAA,oBAAA;AAAA,KAKQ;AAJP,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,SAAA,IAAA,QAAA,CAAA,SAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAIE,GANT,E,CAIgB,mBAAA,CAAA,gEAAA,C,EACf,mBAAA,CAA2D,KAA3D,EAA2D;AAAtD,IAAA,KAAK,EAAC,MAAgD;AAAxC,IAAA,GAAG,EAAE,KAAA,CAAA,IAAmC;AAA7B,IAAA,KAA0B,EAA1B;AAAA,qBAAA;AAAA;AAA6B,GAA3D,E,IAAA,E;;AAAA,I,UAAA,C,CALD,CAOK,CAzBN,C,EA2BS,mBAAA,CAAA,gCAAA,C,EACT,mBAAA,CAEM,KAFN,EAEM,IAFN,EAEM,CADL,mBAAA,CAA0D,QAA1D,EAA0D;AAAlD,IAAA,EAAE,EAAC,aAA+C;AAAhC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,QAAA,IAAA,QAAA,CAAA,QAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA2B,GAA1D,EAA2C,QAA3C,CACK,CAFN,C,EAIS,mBAAA,CAAA,oCAAA,C,EACT,mBAAA,CAOM,KAPN,EAAA,WAAA,EAOM,CANO,mBAAA,CAAA,wBAAA,CAMP,EALL,mBAAA,CAAwD,KAAxD,EAAwD;AAAnD,IAAA,KAAK,EAAC,MAA6C;AAAtC,IAAA,EAAE,EAAC,iBAAmC;AAAhB,IAAA,GAAG,EAAE,KAAA,CAAA;AAAW,GAAxD,E,IAAA,E;;AAAA,I,WAAA,CAKK,EAHO,mBAAA,CAAA,sDAAA,CAGP,EAFO,mBAAA,CAAA,0BAAA,CAEP,EADL,WACK,CAPN,C,CAzDF,E;;AAAA,G","sourcesContent":["<template>\r\n\t<div class=\"FazerLogin\" :class=\"{ FazerLogin2 : tipoPatoEscritor, FazerLogin3 : tipoPatoSenha}\" >\r\n        <!-- Componentente para fazer login -->\r\n\t\t<h2 style=\"font-weight: 600; font-size: 1.2em; margin-left: 3%\">Bem-vindo</h2>\r\n\t\t<h4 style=\"font-weight: 500; margin-left: 5%\">Acesse seus <label style=\"color: #ffd666\">laudos</label> agora!</h4>\r\n\r\n        <!-- Formulário de informações do Login -->\r\n\t\t<form>\r\n            <!-- Primeiro Input: CPF -->\r\n\t\t\t<div class=\"GrupoLogin\">\r\n\t\t\t\t<img class=\"imgs\" :src=\"pessoa\"/>\r\n\t\t\t\t<input \r\n \t\t\t\t\tv-maska=\"'###.###.###-##'\"\r\n\t\t\t\t\ttype=\"text\"\r\n\t\t\t\t\tclass=\"ControleLogin\"\r\n\t\t\t\t\tv-model=\"cpf\"\r\n\t\t\t\t\tplaceholder=\"Escreva aqui seu CPF\"\r\n\t\t\t\t\t@input=\"trocaPatoEscritor()\"\r\n\t\t\t\t\t@change=\"trocaPatoParado()\"\r\n\t\t\t\t/>\r\n\t\t\t</div>\r\n\r\n\t\t\t</form>\r\n\r\n            <!-- Div com o input da senha e do botão para confirmar o login -->\r\n\t\t\t<div class=\"GrupoLogin\">\r\n\r\n                <!-- Imagem de uma chave -->\r\n\t\t\t\t<img class=\"imgs\" :src=\"chave\" />\r\n\r\n                <!-- Segundo Input: Senha -->\r\n\t\t\t\t<label for=\"senhaLogin\"></label>\r\n\t\t\t\t<input\r\n\t\t\t\t\t:type=\"typeSenha\"\r\n\t\t\t\t\tclass=\"ControleLogin\"\r\n\t\t\t\t\tv-model=\"senha\"\r\n\t\t\t\t\tplaceholder=\"Escreva aqui sua senha\"\r\n\t\t\t\t\tid=\"senhaLogin\"\r\n\t\t\t\t\t@input=\"trocaPatoSenha()\"\r\n\t\t\t\t\t@change=\"trocaPatoParado()\"\r\n\t\t\t\t/>\r\n\r\n                <!-- Botão do olho de mostrar/ocultar senha -->\r\n\t\t\t\t<button\r\n\t\t\t\t\tstyle=\"width: 15%; border: none; background: none\"\r\n\t\t\t\t\t@click=\"trocaOlho\"\r\n\t\t\t\t>\r\n                    <!-- Imagem do olho que muda conforme está visivel ou não a senha -->\r\n\t\t\t\t\t<img class=\"imgs\" :src=\"olho\" style=\"padding-top: 0.3vh\" />\r\n\t\t\t\t</button>\r\n\t\t\t</div>\r\n\r\n            <!-- Botão para confirmar o login -->\r\n\t\t\t<div>\r\n\t\t\t\t<button id=\"LoginEntrar\" @click=\"trocaPag\">Entrar</button>\r\n\t\t\t</div>\r\n\r\n            <!-- Divisória do Esqueci minha senha -->\r\n\t\t\t<div id=\"LoginCadeado\">\r\n                <!-- Imagem de um cadeado -->\r\n\t\t\t\t<img class=\"imgs\" id=\"LoginCadeadoImg\" :src=\"cadeado\" />\r\n\r\n                <!-- Texto com uma parte clickavel para recuperar senha -->\r\n                <!-- AINDA NÃO IMPLEMENTADO -->\r\n\t\t\t\t<p>Esqueceu sua senha? <a>Clique aqui</a></p>\r\n\t\t\t</div>\r\n\t\t\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\r\n// Exporta os arquivos do componente\r\nexport default {\r\n    // Nome do componente\r\n\tname: \"FazerLogin\",\r\n\r\n    // A data contêm as variaveis e informações do componente\r\n\tdata() {\r\n\t\treturn {\r\n\t\t\tpessoa: \"./img/Pessoa.svg\",\r\n\t\t\tchave: \"./img/Chave.svg\",\r\n\t\t\tcadeado: \"./img/Cadeado.svg\",\r\n\t\t\tolho: \"./img/OlhoFechado.svg\",\r\n\t\t\ttypeSenha: \"password\",\r\n\t\t\tsenha: \"\",\r\n\t\t\tduckIdle: '/img/duckIdle.png',\r\n\t\t\ttipoPatoEscritor: false,\r\n\t\t\ttipoPatoSenha: false\r\n\t\t};\r\n\t},\r\n\r\n    // Os methods contêm as funções do componente\r\n\tmethods: {\r\n        // O trocaOlho faz 2 coisas:\r\n        // Ele troca a imagem do olho para aberto caso a senha esteja legivel\r\n        // Ou ele troca para o olho fechado caso a senha esteja ilegivel\r\n        // E por fim ele ainda troca o tipo da senha para legivel ou ilegivel \r\n\t\ttrocaOlho() {\r\n\r\n            // Ele compara o typeSenha para saber se ele está como senha ou não\r\n\t\t\tif (this.typeSenha == \"password\") {\r\n\r\n                // Caso ele esteja como uma senha ele troca para texto\r\n\t\t\t\tthis.typeSenha = \"text\";\r\n\r\n                // E altera o olho para um olho aberto ao invés de fechado\r\n\t\t\t\tthis.olho = \"./img/OlhoAberto.svg\";\r\n\r\n\t\t\t} else {\r\n\r\n                // Se ele não estiver como senha, troca-se o tipo dela para senha\r\n\t\t\t\tthis.typeSenha = \"password\";\r\n\r\n                // E assim o olho se fecha também, para completar o ciclo infinito\r\n\t\t\t\tthis.olho = \"./img/OlhoFechado.svg\";\r\n\t\t\t}\r\n\r\n\t\t},\r\n\r\n        // O trocaPag ele basicamente vai chamar o router para alterar a pagina\r\n\t\ttrocaPag() {\r\n\r\n            // IMPLEMENTAR UM VERIFICADOR DO BANCO DE DADOS PARA REDIRECIONAR PARA O URL CERTO\r\n\r\n            // Ele troca a URL  \r\n\t\t\tthis.$router.push({ path: \"/Login/Adm\" });\r\n\t\t},\r\n\r\n        // O cleanString(value) limpa a string do CPF para pegar somente os numeros do CPF\r\n        // E esse (value) é a variavel do CPF.\r\n\t\tcleanString(value) {\r\n\t\t\treturn value.replace(/[&\\/\\\\#,+()$~%.'\":*?<>{}-]/g, \"\");\r\n\t\t},\r\n\r\n\t\ttrocaPatoParado() {\r\n\t\t\tthis.tipoPatoEscritor = false;\r\n\t\t\tthis.tipoPatoSenha = false;\r\n\t\t},\r\n\r\n\t\ttrocaPatoEscritor() {\r\n\t\t\tthis.tipoPatoEscritor = true;\r\n\t\t\tthis.tipoPatoSenha = false;\r\n\t\t},\r\n\r\n\t\ttrocaPatoSenha() {\r\n\t\t\tif (this.typeSenha == 'password'){\r\n\t\t\t\tthis.tipoPatoSenha = true;\r\n\t\t\t\tthis.tipoPatoEscritor = false;\r\n\t\t\t}else{\r\n\t\t\t\tthis.tipoPatoEscritor = true;\r\n\t\t\t\tthis.tipoPatoSenha = false;\r\n\t\t\t}\r\n\t\t}\r\n\r\n\t},\r\n};\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n* {\r\n\tcolor: #ffffff;\r\n}\r\n\r\n#gridDuck{\r\n\tdisplay: grid;\r\n\tgrid-template-columns: 1% 99%;\r\n}\r\n\r\n.FazerLogin{\r\n\tbackground-image: url('/img/duckIdle.png');\r\n\tbackground-repeat: no-repeat;\r\n\tpadding-top: 4vh;\r\n\ttext-align: center;\r\n}\r\n\r\n.FazerLogin2{\r\n\tbackground-image: url('/img/duckWrite.gif');\r\n\tbackground-repeat: no-repeat;\r\n\tpadding-top: 4vh;\r\n\ttext-align: center;\r\n}\r\n\r\n.FazerLogin3{\r\n\tbackground-image: url('/img/duckEyeClosed.png');\r\n\tbackground-repeat: no-repeat;\r\n\tpadding-top: 4vh;\r\n\ttext-align: center;\r\n}\r\n\r\nform {\r\n\tpadding-top: 3vh;\r\n}\r\n\r\n.GrupoLogin {\r\n\tdisplay: flex;\r\n\tmargin: 2vh 2vw 0 2vw;\r\n\theight: 6vh;\r\n\r\n\tbackground-color: #f5f5f5;\r\n\tborder: 1px solid #2e4a7d;\r\n\tbox-sizing: border-box;\r\n\tborder-radius: 8px;\r\n}\r\n\r\n.ControleLogin {\r\n\tfont-weight: 400;\r\n\r\n\tcolor: rgb(94, 94, 94);\r\n\tbackground-color: #f5f5f5;\r\n\toutline: none;\r\n\tborder: none;\r\n\theight: 5.6vh;\r\n\twidth: 14.8vw;\r\n}\r\n\r\n.imgs {\r\n\talign-self: center;\r\n\tmax-height: 3.3vh;\r\n\tmax-width: 3.3vw;\r\n\tpadding-left: 0.5vw;\r\n\tpadding-right: 0.5vw;\r\n}\r\n\r\n#LoginEntrar {\r\n\tmargin-top: 3vh;\r\n\theight: 6vh;\r\n\twidth: 19vw;\r\n\tcursor: pointer;\r\n\tfont-size: 20px;\r\n\tfont-weight: 700;\r\n\tcolor: #2e4a7d;\r\n\tbackground-color: #ffd666;\r\n\tborder-radius: 8px;\r\n\tborder: 1.5px solid #ffd666;\r\n\ttransition: .3s ease all;\r\n}\r\n\r\n#LoginEntrar:hover{\r\n\tborder: 1.5px solid #fff;\r\n\tcolor: #fff;\r\n\tbackground-color: #2e4a7d;\r\n}\r\n\r\n#LoginCadeado {\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n\tpadding-top: 1vh;\r\n}\r\n\r\n#LoginCadeadoImg {\r\n\tmax-height: 3vh;\r\n\tmax-width: 3vw;\r\n}\r\n\r\np {\r\n\tfont-size: 0.9rem;\r\n}\r\n\r\na {\r\n\tcolor: #ffd666;\r\n\ttext-decoration: underline #ffd666;\r\n\tcursor: pointer;\r\n\ttransition: .3s ease all;\r\n}\r\n\r\na:hover{\r\n\tcolor: #2e4a7d;\r\n\tfont-weight: 500;\r\n\tfont-size: 0.94rem;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":""}]}